{
  "name": "@stratawp/explorer",
  "version": "0.7.0",
  "description": "Interactive component explorer and documentation browser for StrataWP themes",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "wordpress",
    "gutenberg",
    "components",
    "storybook",
    "documentation",
    "explorer",
    "stratawp"
  ],
  "author": "StrataWP",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "express": "^4.18.2",
    "vite": "^5.0.0",
    "chokidar": "^3.5.3",
    "ws": "^8.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "open": "^10.0.0",
    "fs-extra": "^11.2.0",
    "fast-glob": "^3.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/fs-extra": "^11.0.4",
    "@vitejs/plugin-react": "^4.2.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
